<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_fise_keyguard"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context="com.android.fisewatchlauncher.acty.FiseKeyguardActivity">

    <!-- android:orientation="vertical" -->

    <com.android.fisewatchlauncher.widget.FiseRectAnalogClockOne
        android:id="@+id/analog_clock_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:background="@null" />

    <com.android.fisewatchlauncher.widget.FiseRectAnalogClockThree
        android:id="@+id/analog_clock_3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:background="@null"
        android:visibility="gone" />

    <com.android.fisewatchlauncher.widget.FiseRectAnalogClockFour
        android:id="@+id/analog_clock_4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:background="@null"
        android:visibility="gone" />

    <com.android.fisewatchlauncher.widget.FiseRectAnalogClockFive
        android:id="@+id/analog_clock_5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:background="@null"
        android:visibility="gone" />


</RelativeLayout>